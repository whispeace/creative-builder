{
  "name": "pnpm-monorepo",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.5",
  "description": "Example pnpm monorepo",
  "keywords": [
    "momorepo",
    "pnpm",
    "typescript",
    "vite",
    "vue",
    "tailwindcss",
    "primevue"
  ],
  "scripts": {
    "carousel-builder#dev": "pnpm run --filter carousel-builder dev",
    "carousel-builder#build": "pnpm run --filter carousel-builder build",
    "carousel-builder#preview": "pnpm run --filter carousel-builder preview",
    "vpaid-builder#dev": "pnpm run --filter vpaid-builder dev",
    "vpaid-builder#build": "pnpm run --filter vpaid-builder build",
    "vpaid-builder#preview": "pnpm run --filter vpaid-builder preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.9.0",
    "@types/node": "^20.11.30",
    "eslint": "^8.57.0",
    "typescript": "5.4.3"
  }
}
